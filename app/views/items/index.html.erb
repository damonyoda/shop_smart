            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            All items
                        </div>
                        
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <table id= "items" width="100%" class="table table-striped table-bordered table-hover sssss">
                                
                                <thead>
                                    <tr>
        <th>Item name</th>
        <th>Item info</th>
        <th>Mall</th>
        <th>Available in</th>
        <th>Price</th>

    </tr>
    </thead>
    <tbody>
<% @item_list_comparison.each do |item_name, item| %>
<% item.each do |i| %>
    <tr>
        <td><%= link_to i.item_name, i %></td>
        <td><%= i.item_info %></td>
        <td><%= i.mall_name %></td>
        <td><%= i.available_in %></td>
        <td><%= i.item_price %></td>
    </tr>
    <% end %>
     <% end %>
     </tbody>
    </table>
    </div>
    </div>
    </div>
